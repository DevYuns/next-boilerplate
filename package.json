{
  "name": "next-proj",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "relay": "relay-compiler",
    "relay-watch": "yarn relay-compiler --watch",
    "codegen": "graphql-codegen --config codegen.yml",
    "generate:client": "yarn codegen && yarn relay",
    "generate:nexus": "ts-node --transpile-only ./server/schema",
    "generate:prisma": "prisma generate --schema=./server/prisma/schema.prisma",
    "generate:server": "yarn generate:nexus && yarn generate:prisma"
  },
  "dependencies": {
    "@apollo/server": "^4.3.2",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@next/font": "13.1.6",
    "@prisma/client": "4.9.0",
    "graphql": "^16.6.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-middleware": "^6.1.33",
    "next": "13.1.6",
    "nexus": "^1.3.0",
    "prisma": "^4.9.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-relay": "^14.1.0",
    "relay-runtime": "^14.1.0",
    "tiny-invariant": "^1.3.1",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^3.0.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/react-relay": "^14.1.3",
    "@types/relay-test-utils": "^14.1.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "relay-compiler": "^14.1.0",
    "relay-compiler-language-typescript": "^15.0.1",
    "relay-config": "^12.0.1",
    "relay-test-utils": "^14.1.0",
    "ts-node": "^10.9.1"
  },
  "prisma": {
    "seed": "ts-node --transpile-only ./prisma/seed.ts"
  }
}
